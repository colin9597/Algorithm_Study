   "source": [
    "# K개의 정수\n",
    "K = int(input())\n",
    "\n",
    "# 장부\n",
    "cog = []\n",
    "for _ in range(K):\n",
    "    n = int(input())\n",
    "    # 0이 아니면 장부에 입력\n",
    "    if n != 0:\n",
    "        cog.append(n)\n",
    "    # 0이면 최근에 쓴 수 제거\n",
    "    else:\n",
    "        cog.pop()\n",
    "        \n",
    "# 장부에 적어 낸 수의 합 출력\n",
    "print(sum(cog))"
   ]
  }
